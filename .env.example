# Database (REQUIRED - no default)
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5433/canpoli
DATABASE_POOL_SIZE=5
DATABASE_MAX_OVERFLOW=10
DATABASE_POOL_TIMEOUT=30
DATABASE_POOL_RECYCLE=1800
DATABASE_ECHO=false

# House of Commons API
HOC_API_BASE_URL=https://www.ourcommons.ca
HOC_API_TIMEOUT=10.0

# CORS (comma-separated list of allowed origins, empty for development mode)
CORS_ORIGINS=[]

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Application
DEBUG=false
LOG_LEVEL=INFO
